{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    // CommonJS module settings:
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "CommonJS",
    "outDir": "./dist/cjs",
    // We will actually emit code:
    "emitDeclarationOnly": false,
    "declaration": false,
    "composite": false
  }
}
